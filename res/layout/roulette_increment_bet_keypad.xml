        android:orientation="vertical">   
        </LinearLayout>
        <LinearLayout
        android:id="@+id/roulette_txt_1000_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">  
        <TextView
                               android:id="@+id/roulette_txt_1000"
                               android:layout_width="match_parent"
                               android:layout_height="match_parent"/> 
        </LinearLayout>
        <LinearLayout
        android:id="@+id/roulette_empty_after_1000_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">   
        </LinearLayout>
        <LinearLayout
        android:id="@+id/roulette_txt_5000_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
         <TextView
                               android:id="@+id/roulette_txt_5000"
                               android:layout_width="match_parent"
                               android:layout_height="match_parent"/>   
        </LinearLayout>
        <LinearLayout
        android:id="@+id/roulette_empty_after_5000_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">   
        </LinearLayout>
   </LinearLayout>
       <!--  fourth horizontal linear layout for roulette increment betkeys 100,500,1000,5000  ends here -->
    
</LinearLayout>
<!--  roulette increment bet keypad linear layout ends here -->
