        android:orientation="vertical"> 
        <TextView
                               android:id="@+id/roulette_txt_500"
                               android:layout_width="match_parent"
                               android:layout_height="match_parent"/>  
        </LinearLayout>
        <LinearLayout
        android:id="@+id/roulette_empty_after_500_layout"
        android:layout_width="match_parent"
